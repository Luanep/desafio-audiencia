{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": "off",
      "no-console": "off",
      "max-len": "off",
      "func-names": "off",
      "no-underscore-dangle": "off",
      "no-use-before-define": "off",
      "quotes": [0, "single"]
    }
}
